<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Promise - then</title>

  <%- renderAgent({}) %>
</head>
<body>
  <h1>Promise - then</h1>

  <button type="button" id="btn1">Button</button>

  <script>
    btn1.addEventListener('click', function () {
      Promise.resolve().then(function (resolve) {
        SplunkRum.provider.getTracer('default').startSpan('context-child').end();
      });
    });
  </script>
</body>
</html>
